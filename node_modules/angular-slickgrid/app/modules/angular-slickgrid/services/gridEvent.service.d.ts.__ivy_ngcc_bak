import { CellArgs, SlickEventHandler } from './../models/index';
export declare class GridEventService {
    private _eventHandler;
    readonly eventHandler: SlickEventHandler;
    constructor();
    dispose(): void;
    bindOnCellChange(grid: any, dataView: any): void;
    bindOnClick(grid: any, dataView: any): void;
    handleOnCellChange(grid: any, dataView: any, e: KeyboardEvent | MouseEvent, args: CellArgs): void;
    private handleOnCellClick;
}
